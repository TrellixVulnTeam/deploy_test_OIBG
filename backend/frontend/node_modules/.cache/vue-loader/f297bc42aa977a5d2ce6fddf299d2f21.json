0vh\"\n            @row-click=\"clickUser\">\n            <el-table-column\n              prop=\"userId\"\n              label=\"ID\"\n              width=\"150\">\n            </el-table-column>\n            <el-table-column\n              prop=\"userName\"\n              label=\"이름\"\n              width=\"150\">\n            </el-table-column>\n            <el-table-column\n              prop=\"userPhone\"\n              label=\"연락처\"\n              width=\"180\">\n            </el-table-column>\n            <el-table-column\n              prop=\"userAddress\"\n              label=\"주소\"\n              width=\"400\">\n            </el-table-column>\n            <el-table-column\n              label=\"탈퇴\">\n              <el-button @click=\"clickDeleteUser\" type=\"danger\">탈퇴</el-button>\n            </el-table-column>\n          </el-table>\n        </el-tab-pane>\n        <el-tab-pane label=\"신고 목록\">\n          <el-table\n            :data=\"state.reportList\"\n            height=\"55vh\"\n            style=\"width: 100%\">\n            <el-table-column\n              prop=\"reporterId\"\n              label=\"신고한 ID\"\n              width=\"180\">\n            </el-table-column>\n            <el-table-column\n              prop=\"reporterTargetId\"\n              label=\"신고된 ID\"\n              width=\"180\">\n            </el-table-column>\n            <el-table-column\n              prop=\"roportDate\"\n              label=\"신고 시간\">\n            </el-table-column>\n          </el-table>\n        </el-tab-pane>\n        <!-- <el-tab-pane label=\"도움 요청 목록\">\n          <el-table\n            :data=\"state.reportList\"\n            height=\"55vh\"\n            style=\"width: 100%\">\n            <el-table-column\n              prop=\"reporterId\"\n              label=\"신고한 ID\"\n              width=\"180\">\n            </el-table-column>\n            <el-table-column\n              prop=\"reporterTargetId\"\n              label=\"신고된 ID\"\n              wid